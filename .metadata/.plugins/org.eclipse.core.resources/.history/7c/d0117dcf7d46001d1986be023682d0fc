#include <msp430.h> 
#include <stdint.h>

/**
 * main.c
 */


int main(void)
{
	WDTCTL = WDTPW | WDTHOLD;	// stop watchdog timer
	
	/*Set Ports*/
	P1DIR |= BIT0;        //set pin P1.0 as output
	P1OUT &= ~(BIT0);     //Clear bit

	int i = 0;
	while(1)
	{
	    P1OUT ^= BIT0;    // Toggle the pin P1.0
	    for(i = 0;i < 0xFFFF; ++i)
	    {
	        //do nothing
	    }

	}

}

void delay_ms(uint16_t time)
{

}
